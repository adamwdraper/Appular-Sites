{
    "utility": {
        "api": {
            "modified": "2012-07-31",
            "description": "makes calls to api and returns data to a callback",
            "file": "test/data/data.js",
            "extends": "backbone.view",
            "define": "est/data/data",
            "doc": {
                "baseUrl": {
                    "type": "string",
                    "default": "window.location.protocol+'//'+window.location.hostname+'/'+'api'",
                    "description": "base url for all api requests"
                },
                "trackData": {
                    "type": "object",
                    "description": "info added to all tracking calls",
                    "property": {
                        "userAgent": {
                            "type": "string",
                            "default": "navigator.userAgent"
                        },
                        "domain": {
                            "type": "string",
                            "default": "window.location.hostname",
                            "description": "current domain"
                        }
                    },
                    "method": {
                        "domainf": {
                            "type": "function",
                            "description": "current domain function",
                            "param": {
                                "something": {
                                    "type": "string",
                                    "isOptional": true,
                                    "description": "fjlksjf lkj f"
                                }
                            },
                            "return": {
                                "somethingelse": {
                                    "type": "boolean",
                                    "description": "faslfjs lkdjf"
                                }
                            }
                        }
                    }
                },
                "ajax": {
                    "modified": "2012-07-31",
                    "type": "function",
                    "description": "make ajax call to api",
                    "param": {
                        "method": {
                            "type": "string",
                            "isOptional": false,
                            "description": "get or post"
                        },
                        "path": {
                            "type": "string",
                            "isOptional": false,
                            "description": "path to resource with leading slash"
                        },
                        "data": {
                            "type": "object",
                            "isOptional": false,
                            "description": "data to send to api",
                            "property": {
                                "data": {
                                    "type": "string||object",
                                    "isOptional": true,
                                    "description": "data to be send with jQuery ajax call"
                                },
                                "userAuth": {
                                    "type": "boolean",
                                    "default": "false",
                                    "isOptional": true,
                                    "description": "whether user signature is sent to api"
                                }
                            }
                        },
                        "callback": {
                            "type": "function",
                            "isOptional": true,
                            "description": "called after ajax",
                            "data": {
                                "type": "object",
                                "isOptional": false,
                                "description": "this will contain data returned by api"
                            },
                            "error": {
                                "type": "object",
                                "isOptional": true,
                                "description": "if this is defined, an error occured with the ajax call"
                            }
                        }
                    }
                }
            }
        }
    }
}